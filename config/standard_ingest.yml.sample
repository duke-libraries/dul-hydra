:scanner:
    :exclude:
        - .DS_Store
        - Thumbs.db
        - metadata.txt
    :targets: dpc_targets
:metadata:
    :csv:
        :encoding: UTF-8
        :headers: true
        :col_sep: "\t"
    :parse:
        :repeating_fields_separator: ;
        :repeatable_fields:
            - contributor
            - coverage
            - creator
            - extent
            - identifier
            - rightsHolder
            - subject
            - temporal
