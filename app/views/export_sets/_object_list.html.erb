<table class="table table-bordered table-striped table-condensed" style="width: auto">
  <thead>
	<tr>
	  <% if @export_set.new_record? -%>
	    <th scope="col">#</th>
	  <% end -%>
	  <% object_list_cols.each do |col| -%>
	    <th scope="col"><%= render_object_list_header col %></th>
	  <% end -%>
	</tr>
  </thead>
  <tbody>
	<% export_set_object_list.each do |obj| %>
	<tr>
	  <% if @export_set.new_record? -%>
	    <td><%= check_box_tag 'export_set[pids][]', obj.pid, true %></td>
	  <% end -%>
	  <% object_list_cols.each do |col| -%>
	    <td><%= render_object_list_row_value obj, col %></td>
	  <% end -%>
	</tr>
	<% end %>
  </tbody>
</table>
