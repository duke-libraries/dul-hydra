<% if resource.new_record? -%>
  <% if required?(resource, :admin_policy) -%> 
    <%= render 'select_admin_policy' %>
  <% end -%>
  <% if required?(resource, :content) -%>
    <%= render 'select_content_file' %>
  <% end -%>
  <% if params[:attached_to_id] %>
    <%= hidden_field_tag :attached_to_id, params[:attached_to_id] %>
  <% end %>
<% end -%>
