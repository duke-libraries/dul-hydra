<div class="navbar">
  <div class="navbar-inner">
	<ul class="nav">
	  <% if can? :edit, current_object -%>
	  <li>
		<%= link_to "Edit Default Permissions", default_permissions_edit_path(current_object) %>
	  </li>
	  <% end -%>
	  <% if can? :download, current_object.defaultRights -%>
	    <li>
		  <%= download_datastream_xml_link 'defaultRights' %>
	    </li>
	  <% end -%>
	</ul>
  </div>
</div>

<%= render 'permissions/default_permissions_alert' %>

<%= render partial: 'access_controls', locals: {access_map: default_permissions_access_map} %>

<%= render partial: 'license', locals: {license: current_object.default_license} %>
