<h2>New AdminPolicy</h2>

<%= render partial: 'new_errors', locals: {new_object: @admin_policy} %>

<%= form_for @admin_policy do |f| %>
  <%= f.label :title %>
  <%= text_field_tag "admin_policy[title]", nil, required: true %>

  <%= f.label :description %>
  <%= text_area_tag "admin_policy[description]" %>

  <%= render partial: 'new_form_actions', locals: {f: f, new_object: @admin_policy} %>
<% end %>
