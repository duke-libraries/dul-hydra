<div class="form-group">
  <%= label_tag "content_file", "Content File" %>
  <%= file_field_tag "content[file]" %>
</div>
<div class="form-group row">
  <div class="col-md-3">
	<%= label_tag "content_checksum_type", "Checksum Type" %>
	<%= select_tag "content[checksum_type]", options_for_select(DulHydra::Datastreams::CHECKSUM_TYPES, "SHA-256"), class: "form-control" %>
  </div>
  <div class="col-md-9">
	<%= label_tag "content_checksum", "Checksum" %>
	<%= text_field_tag "content[checksum]", nil, class: "form-control" %>
  </div>
</div>
