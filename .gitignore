# Ignore bundler config
/.bundle

# Extras
/contrib

# Ignore the default SQLite database.
/db/*.sqlite3

/jetty

# Ignore all logfiles and tempfiles.
/log/*
/tmp

# Ignore all config yaml files
/config/*.yml
/config/metadata_file_profiles/*.yml

# Environment configs
/config/environments/*.rb

# SimpleCov / Coveralls
/coverage

# Pre-compiled assets
/public/assets

# Paperclip files
/public/system

# Default python virtual environment
/python

.sass-cache

# Ignore Mac .DS_Store files
.DS_Store

# fedora-migrate
/migration_report
/test/integration/migration/fixtures/f3-migration-jetty-*.zip
